                    <li class="dropdown-header text-start">
                      <h6>Filter</h6>
                    </li>
                    
                    <li>
                      <a class="dropdown-item" href="javascript:TotalValueCardReload('@Url.Action("FitlerTotalClasses", "Home", new { ByTime = "" })')">
                        Today
                      </a>
                    </li>
                    <li>
                      
                      <a class="dropdown-item" href="javascript:TotalValueCardReload('@Url.Action("FitlerTotalClasses", "Home", new { ByTime = ByTime.Month })')">
                        This Month
                      </a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="javascript:TotalValueCardReload('@Url.Action("FitlerTotalClasses", "Home", new { ByTime = ByTime.Year })')">
                        This Year
                      </a>
                    </li>
                    
                  </ul>
                </div>

                <div class="card-body">
                  <partial name="_TotalClasses" model="Model.ClassTotalValueModel" />
                </div>

              </div>
            </div><!-- End Revenue Card -->
            <!-- Tutors Card -->
            <div class="col-xxl-4 col-md-6">
