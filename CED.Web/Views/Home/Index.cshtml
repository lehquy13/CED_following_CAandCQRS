            <!-- Reports -->
            <div class="col-12">
              <div class="card">

                <div class="filter">
                  <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                  <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                    <li class="dropdown-header text-start">
                      <h6>Filter</h6>
                    </li>
                    <li>
                      <a class="dropdown-item"
                         href="javascript:ChartReload('@Url.Action("FitlerLineChart", "Home", new { ByTime = "week" })')">
                        This Week
                      </a>
                    </li>
                    <li>
                      
                      <a class="dropdown-item"
                         href="javascript:ChartReload('@Url.Action("FitlerLineChart", "Home", new { ByTime = "month" })')">
                        This Month
                      </a>
                    </li>
                  </ul>
                </div>

                <div class="card-body">
                  <h5 class="card-title">Reports </h5>

                  <!-- Line Chart -->
                  <div id="reportsChart"></div>

                  <script>
                    document.addEventListener("DOMContentLoaded", () => {
                       window.reportsChartStore = new ApexCharts(document.querySelector("#reportsChart"), {
                        series: @Html.Raw(Model.ChartWeekData),
                        id:'reportsChart1',
                        chart: {
                          height: 350,
                          type: 'area',
                          toolbar: {
                            show: false
                          },
                        },
                        markers: {
                          size: 4
                        },
                        colors: ['#4154f1', '#2eca6a', '#ff771d'],
                        fill: {
                          type: "gradient",
                          gradient: {
                            shadeIntensity: 1,
                            opacityFrom: 0.3,
                            opacityTo: 0.4,
                            stops: [0, 90, 100]
                          }
                        },
                        dataLabels: {
                          enabled: false
                        },
                        stroke: {
                          curve: 'smooth',
                          width: 2
                        },
                        xaxis: @Html.Raw(@Model.DatesWeekData),
                        tooltip: {
                          x: {
                            format: 'dd/MM/yy HH:mm'
                          },
                        }
                      });
                      reportsChartStore.render();
                    });
                  </script>
                  <!-- End Line Chart -->

                </div>

              </div>
            </div><!-- End Reports -->

            <!-- Recent Classes -->
            <div class="col-12">
              <div class="card recent-sales overflow-auto">

                <div class="filter">
                  <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                  <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                    <li class="dropdown-header text-start">
                      <h6>Filter</h6>
                    </li>

                    <li><a class="dropdown-item" href="#">Today</a></li>
                    <li><a class="dropdown-item" href="#">This Month</a></li>
                    <li><a class="dropdown-item" href="#">This Year</a></li>
                  </ul>
                </div>

                <div class="card-body">
                  <h5 class="card-title">Recent Classes <span>| Today</span></h5>

                  <table class="table table-striped ">
                    <thead>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">Title</th>
                        <th scope="col">Learning Mode</th>
                        <th scope="col">Charge Fee</th>
                        <th scope="col">Status</th>
                      </tr>
                    </thead>
                    <tbody>
                     @foreach (var classInfo in Model.ClassTotalValueModel.Models.Where(x => x.Status == Status.Waiting))
                     {
                       @if (i > 4)
                       {
                         break;
                       }
                       <tr>
                         <th scope="row" class="text-primary">
                           @i
                         </th>
                         <td>
                           <a href="@Url.Action("Edit","ClassInformation",new {id =classInfo.Id})" class="text-primary">@classInfo.Title</a>
                         </td>
                         <td>@classInfo.LearningMode</td>
                         <td>
                           <a href="@Url.Action("Edit", "ClassInformation", new {id =classInfo.Id})" class="text-primary">
                             $@classInfo.ChargeFee
                           </a>
                           </td>
                         @switch (classInfo.Status)
                         {
                           case Status.Waiting:
                             <td>
                               <span class="badge bg-primary">@classInfo.Status</span>
                             </td>
                             break;
                           case Status.Canceled:
                             <td>
                               <span class="badge bg-danger">@classInfo.Status</span>
                               
                             </td>
                             break;
                           case Status.Confirmed:
                             <td>
                               <span class="badge bg-success">@classInfo.Status</span>
                               
                             </td>
                             break;
                           case Status.OnConfirming:
                             <td>
                               <span class="badge bg-warning">@classInfo.Status</span>
                               
                             </td>
                             break;
                         } 
                       </tr>
                       i++;
                     }
                    </tbody>
                  </table>
                  <a asp-controller="ClassInformation" asp-action="Index">See more...</a>
                </div>

              </div>
            </div><!-- End Recent Sales -->

